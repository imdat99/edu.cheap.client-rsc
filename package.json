{
  "name": "edu.cheap.client-rsc",
  "private": true,
  "type": "module",
  "scripts": {
    "unocss": "unocss \"src/**/*.{html,vue,js,ts,jsx,tsx}\" -o src/routes/root/style.css -m",
    "build": "vite build",
    "dev": "cross-env NODE_ENV=development vite",
    "start": "cross-env NODE_ENV=production node server.js",
    "bun-start": "bun index.bun.ts",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "dev:cf": "wrangler dev",
    "cf-deploy": "wrangler deploy -c dist/rsc/wrangler.json && wrangler deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts",
    "deploy": "wrangler deploy --minify"
  },
  "dependencies": {
    "@hiogawa/tiny-rpc": "^0.2.3-pre.18",
    "@hiogawa/utils": "^1.7.0",
    "compression": "^1.8.1",
    "cross-env": "^10.1.0",
    "express": "^5.1.0",
    "hono": "^4.9.11",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-router": "7.9.4",
    "swr": "^2.3.6",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.13.12",
    "@hono/bun-compress": "^0.1.0",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.3",
    "@types/node": "^24.7.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitejs/plugin-rsc": "^0.4.33",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.9.3",
    "unocss": "^66.5.3",
    "vite": "^7.1.9",
    "vite-plugin-devtools-json": "1.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.42.2"
  }
}
