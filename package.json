{
  "name": "edu.cheap.client-rsc",
  "private": true,
  "type": "module",
  "scripts": {
    "unocss": "unocss \"src/**/*.{html,vue,js,ts,jsx,tsx}\" -o src/routes/root/style.css -m",
    "build": "vite build",
    "dev": "cross-env NODE_ENV=development vite",
    "start": "cross-env NODE_ENV=production node server.js",
    "bun-start": "bun index.bun.ts",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "dev:cf": "wrangler dev",
    "cf-deploy": "wrangler deploy -c dist/rsc/wrangler.json && wrangler deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts",
    "deploy": "wrangler deploy --minify",
    "clean-fbts": "rm -rf .enum_manifest.json .src_manifest.json .source_strings.json src/translatedFbts.json || exit 0",
    "manifest": "fbt-manifest --src src",
    "collect-fbts": "fbt-collect --fbt-common-path ./common_strings.json --pretty --manifest < .src_manifest.json > .source_strings.json",
    "translate-fbts": "fbt-translate --translations translations/en_US.json translations/vi_VN.json --jenkins > src/translatedFbts.json",
    "i18n": "npm run manifest && npm run collect-fbts && npm run translate-fbts",
    "fbtee:all": "pnpm fbtee:collect && pnpm fbtee:translate-multiple && pnpm fbtee:translate",
    "fbtee:collect": "fbtee collect --common ./common_strings.json --src src",
    "fbtee:translate": "fbtee translate --stdin < translation_input.json > src/translatedFbts.json",
    "fbtee:translate-multiple": "fbtee translate -o src/translatedFbts"
  },
  "dependencies": {
    "@hiogawa/tiny-rpc": "^0.2.3-pre.18",
    "@hiogawa/utils": "^1.7.0",
    "@remix-run/node-fetch-server": "0.10.0",
    "compression": "^1.8.1",
    "cross-env": "^10.1.0",
    "express": "^5.1.0",
    "fbt": "^1.0.2",
    "fbtee": "^1.7.0",
    "hono": "^4.9.10",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-router": "7.9.4",
    "swr": "^2.3.6",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.13.11",
    "@fbtjs/default-collection-transform": "^1.0.0",
    "@hono/bun-compress": "^0.1.0",
    "@nkzw/babel-preset-fbtee": "^1.7.0",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.3",
    "@types/fbt": "^1.0.5",
    "@types/node": "^24.7.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitejs/plugin-rsc": "^0.4.33",
    "babel-plugin-fbt": "^1.0.0",
    "babel-plugin-fbt-runtime": "^1.0.0",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "graceful-fs": "^4.2.11",
    "typescript": "^5.9.3",
    "unocss": "^66.5.2",
    "vite": "npm:rolldown-vite@latest",
    "vite-plugin-devtools-json": "1.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.42.1"
  }
}
