{
  "name": "edu.cheap.client-rsc",
  "private": true,
  "type": "module",
  "scripts": {
    "unocss": "unocss \"src/**/*.{html,vue,js,ts,jsx,tsx}\" -o src/routes/root/style.css -m",
    "build": "vite build",
    "dev": "cross-env NODE_ENV=development vite",
    "start": "cross-env NODE_ENV=production node server.js",
    "bun-start": "bun index.bun.ts",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "dev:cf": "wrangler dev",
    "cf-deploy": "wrangler deploy -c dist/rsc/wrangler.json && wrangler deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts",
    "deploy": "wrangler deploy --minify"
  },
  "dependencies": {
    "@hiogawa/tiny-rpc": "^0.2.3-pre.18",
    "@hiogawa/utils": "^1.7.0",
    "@remix-run/node-fetch-server": "0.10.0",
    "compression": "^1.8.1",
    "cross-env": "^10.1.0",
    "express": "^5.1.0",
    "hono": "^4.9.10",
    "i18next": "^25.5.3",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "next-i18next": "^15.4.2",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-i18next": "^16.0.0",
    "react-router": "7.9.3",
    "remix-i18next": "^7.4.0",
    "swr": "^2.3.6",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.13.10",
    "@hono/bun-compress": "^0.1.0",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.3",
    "@types/node": "^24.6.2",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitejs/plugin-rsc": "^0.4.32",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "typescript": "^5.9.3",
    "unocss": "^66.5.2",
    "vite": "npm:rolldown-vite@latest",
    "vite-plugin-devtools-json": "1.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.42.0"
  }
}
